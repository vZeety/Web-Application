@{
}


<table border="1">
    <tr>
        <th>Eduid</th>
        <th>Description</th>
        <th>Date</th>
        <th>Major</th>
        <th>Minor</th>
        <th>Is New</th>
        <th>Is Dirty</th>
        
    </tr>

    @{

        @foreach (var c in ViewBag.ListOfEducation)
        {

            <tr>
                <td> @c.Eduid</td>
                <td> @c.Description </td>
                <td> @c.Date </td>
                <td> @c.Major </td>
                <th> @c.Minor</th>
                <td> @c.IsNew </td>
                <td> @c.IsDirty </td>
                
            </tr>

        }


    }
</table>
